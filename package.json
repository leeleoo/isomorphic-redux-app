{
  "name": "isomorphic-redux-app",
  "version": "0.1.1",
  "description": "An example of a isomorphic Redux application",
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "NODE_ENV=development node src/server/index.js --progress --colors --profile",
    "start-prod": "NODE_ENV=production node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors --profile",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caljrimmer/isomorphic-redux-app.git"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.12.0",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "history": "^3.0.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-slick": "^0.12.2",
    "redux": "^3.5.2",
    "redux-router": "^2.0.0",
    "redux-thunk": "^2.1.0",
    "redux-undo": "^0.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jsdom-no-contextify": "3.1.0",
    "merge": "1.2.0",
    "mocha": "^2.5.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0"
  }
}
